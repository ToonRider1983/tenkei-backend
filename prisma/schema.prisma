model TD_Procure {
    Order_No           String // หมายเลขคำสั่ง (Short Text)
    Procure_No         String // หมายเลขการจัดซื้อ (Short Text)
    OdPc_No            String // หมายเลข OdPc (Short Text)
    OdPcLn_No          String   @id // หมายเลข OdPcLn (Short Text)
    Vendor_CAT         String // หมวดหมู่ผู้จำหน่าย (Short Text)
    Vendor_CD          String // รหัสผู้จำหน่าย (Short Text)
    Pc_Name            String // ชื่อการจัดซื้อ (Short Text)
    Pc_Material        String // วัสดุการจัดซื้อ (Short Text)
    Pc_Mate_Docu       String // เอกสารวัสดุการจัดซื้อ (Short Text)
    Temp_Price         String // ราคาชั่วคราว (Price CD)
    Unit_Price         Float // ราคาต่อหน่วย
    Yes_No_1           Boolean // ใช่/ไม่ใช่ 1
    Yes_No_2           Boolean // ใช่/ไม่ใช่ 2
    Short_Text_1       String // ข้อความสั้น 1 (Short Text)
    Number_1           Int // หมายเลข 1 (Number)
    Number_2           Int // หมายเลข 2 (Number)
    Pc_Qty             Int // จำนวนการจัดซื้อ
    Pc_Unit_CD         String // รหัสหน่วยการจัดซื้อ (Short Text)
    Pc_Person_CD       String // รหัสบุคคลที่ทำการจัดซื้อ (Short Text)
    Pc_Date            DateTime // วันที่การจัดซื้อ
    Pc_Req_Delivery    DateTime // วันที่ขอการจัดส่ง
    Pc_Req_Delivery_CD String // รหัสการขอการจัดส่ง (Short Text)
    Pc_Ans_Delivery    DateTime // วันที่ตอบการจัดส่ง
    Pc_Ans_Delivery_CD String // รหัสการตอบการจัดส่ง (Short Text)
    Pc_Delivery_Judge  String // การตัดสินการจัดส่ง (Short Text)
    Pc_Progress_CD     String // รหัสความก้าวหน้า (Short Text)
    Pc_Send_Date       DateTime // วันที่ส่งการจัดซื้อ
    Pc_Arrival_Date    DateTime // วันที่ถึง (Arrival Date)
    Pc_Arrival_Qty     Int // จำนวนที่ถึง (Arrival Quantity)
    Pc_Cancel_Date     DateTime // วันที่ยกเลิก
    Procure_Docu       String // เอกสารการจัดซื้อ (Hyperlink)
    Pc_Remark          String // หมายเหตุการจัดซื้อ (Short Text)
    Print              Boolean // พิมพ์ (Yes/No)
    Pc_Reg_Date        DateTime // วันที่ลงทะเบียนการจัดซื้อ
    Pc_Upd_Date        DateTime // วันที่อัปเดตการจัดซื้อ
    Pc_NAV_Reg_Date    DateTime // วันที่ลงทะเบียน NAV
    PC_NAV_Upd_Date    DateTime // วันที่อัปเดต NAV
